-- Recording System
local recordConnection = nil

local function startRecording()
    if _G.IsRecording then return end
    
    local player = Players.LocalPlayer
    if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
        createNotification("Character not found!", Color3.fromRGB(255, 80, 80))
        return
    end
    
    _G.IsRecording = true
    _G.IsPaused = false
    _G.CurrentRecording = {}
    
    createNotification("Recording Started", Color3.fromRGB(80, 255, 80))
    
    local lastRecord = tick()
    recordConnection = RunService.Heartbeat:Connect(function()
        if not _G.IsPaused and _G.IsRecording then
            if tick() - lastRecord >= _G.RecordInterval then
                local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    table.insert(_G.CurrentRecording, hrp.CFrame)
                    lastRecord = tick()
                end
            end
        end
    end)
end

local function stopRecording()
    if not _G.IsRecording then return end
    
    _G.IsRecording = false
    _G.IsPaused = false
    
    if recordConnection then
        recordConnection:Disconnect()
        recordConnection = nil
    end
    
    createNotification("Recording Stopped", Color3.fromRGB(255, 200, 80))
end

local function pauseRecording()
    if not _G.IsRecording then return end
    
    _G.IsPaused = not _G.IsPaused
    
    if _G.IsPaused then
        createNotification("Recording Paused", Color3.fromRGB(255, 255, 80))
    else
        createNotification("Recording Resumed", Color3.fromRGB(80, 255, 80))
    end
end

local function saveRecording(name)
    if #_G.CurrentRecording == 0 then
        createNotification("No recording data!", Color3.fromRGB(255, 80, 80))
        return false
    end
    
    _G.RecordedPaths[name] = _G.CurrentRecording
    _G.CurrentRecording = {}
    saveToFile()
    createNotification("Saved: " .. name, Color3.fromRGB(80, 255, 80))
    return true
end

local function stopPlayback()
    if _G.PlaybackConnection then
        _G.PlaybackConnection:Disconnect()
        _G.PlaybackConnection = nil
    end
    _G.IsPlaying = false
    createNotification("Playback Stopped", Color3.fromRGB(255, 200, 80))
end

local function playRecording(pathData)
    if _G.IsPlaying then
        createNotification("Already playing!", Color3.fromRGB(255, 80, 80))
        return
    end
    
    local player = Players.LocalPlayer
    if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
        createNotification("Character not found!", Color3.fromRGB(255, 80, 80))
        return
    end
    
    local hrp = player.Character.HumanoidRootPart
    
    _G.IsPlaying = true
    createNotification("Playing Recording", Color3.fromRGB(80, 200, 255))
    
    local index = 1
    local lastUpdate = tick()
    
    _G.PlaybackConnection = RunService.Heartbeat:Connect(function()
        if not _G.IsPlaying or not player.Character then
            stopPlayback()
            return
        end
        
        if tick() - lastUpdate >= _G.RecordInterval then
            if index <= #pathData then
                hrp.CFrame = pathData[index]
                index = index + 1
                lastUpdate = tick()
            else
                stopPlayback()
                createNotification("Playback Complete", Color3.fromRGB(80, 255, 80))
            end
        end
    end)
end

-- Main GUI
local function createMainGUI()
    local gui = Instance.new("ScreenGui")
    gui.Name = "WalkRecordingGUI"
    gui.Parent = game.CoreGui
    gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    gui.ResetOnSpawn = false
    
    -- MINIMIZE ICON (HIDDEN BY DEFAULT)
