        btn.Position = UDim2.new(0, 8, 0, posY)
        btn.BackgroundColor3 = color
        btn.BorderSizePixel = 0
        btn.Text = text
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.Font = Enum.Font.GothamBold
        btn.TextSize = 9
        btn.Parent = main
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 4)
        corner.Parent = btn
        
        return btn
    end
    
    local recordBtn = createButton("âº START", Color3.fromRGB(25, 100, 25), btnY)
    btnY = btnY + btnHeight + btnSpacing
    
    local pauseBtn = createButton("â¸ PAUSE", Color3.fromRGB(100, 100, 25), btnY)
    btnY = btnY + btnHeight + btnSpacing
    
    local stopBtn = createButton("â¹ STOP", Color3.fromRGB(100, 25, 25), btnY)
    btnY = btnY + btnHeight + btnSpacing
    
    -- STOP PLAYBACK BUTTON (NEW!)
    local stopPlayBtn = createButton("â¹ STOP PLAY", Color3.fromRGB(150, 50, 25), btnY)
    btnY = btnY + btnHeight + btnSpacing
    
    -- Input Frame
    local inputFrame = Instance.new("Frame")
    inputFrame.Size = UDim2.new(0, 145, 0, 26)
    inputFrame.Position = UDim2.new(0, 8, 0, btnY)
