
_G.RecordedPaths = {}
_G.IsRecording = false
_G.IsPaused = false
_G.CurrentRecording = {}
_G.RecordInterval = 0.1
_G.IsPlaying = false
_G.PlaybackConnection = nil

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local HttpService = game:GetService("HttpService")
local UIS = game:GetService("UserInputService")

-- Save/Load Functions
local function saveToFile()
    if not writefile then
        warn("Exploit doesn't support writefile!")
        return false
    end
    
    local data = {
        paths = _G.RecordedPaths
    }
    
    local success, err = pcall(function()
        writefile("walk_recording.json", HttpService:JSONEncode(data))
    end)
    
    if success then
        print("[Recording] âœ“ Saved successfully!")
        return true
    else
        warn("[Recording] âœ— Failed:", err)
        return false
    end
end

local function loadFromFile()
    if not readfile or not isfile then
        warn("Exploit doesn't support readfile/isfile!")
        return false
    end
    
    if isfile("walk_recording.json") then
        local success, result = pcall(function()
            local data = HttpService:JSONDecode(readfile("walk_recording.json"))
            _G.RecordedPaths = data.paths or {}
            
            local count = 0
            for _ in pairs(_G.RecordedPaths) do count = count + 1 end
            
            print("[Recording] âœ“ Loaded", count, "recordings")
            return true
        end)
        
        if success then
            return result
        else
            warn("[Recording] âœ— Failed to load save file!")
            return false
        end
    else
        print("[Recording] No save file found")
        return false
    end
end

-- Notification
local function createNotification(text, color)
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "RecordNotif"
    screenGui.Parent = game.CoreGui
    screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 200, 0, 50)
    frame.Position = UDim2.new(0.5, -100, 0.1, -25)
    frame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
    frame.BorderColor3 = color or Color3.fromRGB(80, 255, 80)
    frame.BorderSizePixel = 2
    frame.BackgroundTransparency = 1
    frame.Parent = screenGui
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = frame
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = Color3.fromRGB(255, 255, 255)
    label.Font = Enum.Font.GothamBold
    label.TextSize = 12
    label.Parent = frame
    
    local fadeIn = TweenService:Create(frame, TweenInfo.new(0.3), {BackgroundTransparency = 0})
    fadeIn:Play()
    
    wait(2)
    
    local fadeOut = TweenService:Create(frame, TweenInfo.new(0.3), {BackgroundTransparency = 1})
    fadeOut:Play()
    fadeOut.Completed:Connect(function()
        screenGui:Destroy()
    end)
end

